<?xml version="1.0" encoding="utf-8"?>
<!-- XML for creating statelist for menu buttons. Defines image button should show
on pressed, focused, and default states. -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/menubutton_pressed"
          android:state_pressed="true" />
    <item android:drawable="@drawable/menubutton_focused"
          android:state_focused="true" />
    <item android:drawable="@drawable/menubutton_default" />
</selector>